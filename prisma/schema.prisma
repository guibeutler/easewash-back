generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Client {
  id       String @id @default(uuid())
  name     String
  email    String @unique @db.VarChar(255)
  password String
  phone    String
  street   String
  number   Int
  city     String
  state    String
  zipCode  String
}
